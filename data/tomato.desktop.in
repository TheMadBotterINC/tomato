[Desktop Entry]
Type=Application
Name=@APP_NAME@
Version=@VERSION@
_Comment=@COMMENT@
_GenericName=@GENERIC_NAME@
StartupWMClass=@EXEC_NAME@
Exec=@EXEC_NAME@
Icon=@ICON_NAME@
Terminal=false
Categories=GNOME;GTK;Utility;Productivity
Keywords=Pomodoro;Time;Productivity;Tomato
